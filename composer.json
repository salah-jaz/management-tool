{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "alexusmai/laravel-file-manager": "^3.3",
        "anhskohbo/no-captcha": "^3.7",
        "aws/aws-sdk-php": "^3.296",
        "barryvdh/laravel-dompdf": "^3.1.1",
        "google/apiclient": "^2.18",
        "guzzlehttp/guzzle": "^7.0",
        "laravel/framework": "^11.0",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^11.0",
        "laravel/tinker": "^2.8",
        "laravel/ui": "^4.3",
        "laraveldaily/laravel-invoices": "^4.1",
        "league/flysystem-aws-s3-v3": "^3.22",
        "maatwebsite/excel": "^3.1.63",
        "munafio/chatify": "^1.6",
        "protonemedia/laravel-cross-eloquent-search": "^3.2",
        "psr/container": "^2.0",
        "ryangjchandler/laravel-comments": "^1.0.0",
        "silviolleite/laravelpwa": "^2.0",
        "spatie/laravel-medialibrary": "^11.0",
        "spatie/laravel-permission": "^6.0",
        "twilio/sdk": "^8.1"
    },
    "require-dev": {
        "fakerphp/faker": "^1.21",
        "knuckleswtf/scribe": "^4.40",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.30",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "phpunit/phpunit": "^11.0",
        "spatie/laravel-ignition": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/",
            "Plugins\\": "plugins/"
        },
        "files": [
            "app/app_helpers.php",
            "app/Overrides/ChatifyMessenger.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan optimize:clear"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
